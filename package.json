{
  "name": "timebeacon-app",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "vite --port 3002",
    "dev:backend": "node server-unified.js",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "build:qa": "vite build --mode qa",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "start": "node serve.js",
    "start:qa": "NODE_ENV=staging node serve.js",
    "start:prod": "NODE_ENV=production node serve.js",
    "deploy": "npm run build && npm start",
    "deploy:qa": "npm run build:qa && npm run start:qa",
    "deploy:prod": "npm run build:prod && npm run start:prod",
    "test": "vitest --exclude '**/e2e/**'",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "googleapis": "^159.0.0",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "vitest": "^3.2.4"
  }
}
